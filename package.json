{
  "name": "coup",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "mocha tests/**/*.js",
    "build": "NODE_ENV=production node node_modules/browserify/bin/cmd.js -g envify -g uglifyify client/main.jsx -o client/bundle.js",
    "start": "nodemon ./app",
    "watch": "NODE_ENV=development node node_modules/watchify/bin/cmd.js --debug -p livereactload -g envify client/main.jsx -o client/bundle.js -v",
    "debug": "DEBUG=socket.io:client node app"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "~3.4.0",
    "classnames": "^2.2.5",
    "envify": "^3.4.0",
    "express": "^4.4.3",
    "fluxxor": "~1.7.3",
    "keymirror": "~0.1.1",
    "node-uuid": "~1.4.1",
    "react": "~15.0.0",
    "react-dom": "^15.1.0",
    "shuffle": "~0.2.1",
    "socket.io": "^1.4.6"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "connect-livereload": "^0.5.4",
    "envify": "^3.4.0",
    "livereactload": "^2.2.3",
    "livereload": "^0.4.1",
    "mocha": "^2.5.2",
    "react-proxy": "^1.1.8",
    "uglifyify": "^3.0.1",
    "watchify": "~3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "env": {
            "development": {
              "plugins": [
                [
                  "react-transform",
                  {
                    "transforms": [
                      {
                        "transform": "livereactload/babel-transform",
                        "imports": [
                          "react"
                        ]
                      }
                    ]
                  }
                ]
              ]
            }
          }
        }
      ],
      "envify"
    ]
  }
}
